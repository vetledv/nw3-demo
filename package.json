{
  "name": "nw3-demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "bun:dev": "bunx --bun vite",
    "bun:build": "bunx --bun vite build",
    "build": "vite build",
    "server": "bun --hot run server/index.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "clean": "git clean -fdx node_modules dist src/graphql/gen",
    "prettier": "npx prettier . --write",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "eslint . --ext ts,tsx --report-unused-disable-directives",
      "prettier --write **/*.{js,jsx,ts,tsx}"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@turf/turf": "^6.5.0",
    "@types/mapbox-gl": "^2.7.13",
    "graphql": "^16.8.0",
    "mapbox-gl": "^2.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.5",
    "subscriptions-transport-ws": "^0.11.0",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@types/node": "^20.5.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.15",
    "bun-types": "^1.0.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-graphql-codegen": "^3.3.0"
  }
}
